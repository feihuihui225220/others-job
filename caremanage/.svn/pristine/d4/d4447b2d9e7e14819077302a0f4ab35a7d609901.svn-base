package com.anticw.aged.vo.professional.device;

import java.util.List;

/**
 * 设备分组信息
 * 指定设备类型的设备分组信息
 * <P>File name : DeviceGroupVO.java </P>
 * <P>Author : zhouyanxin </P> 
 * <P>Date : 2014-10-7 </P>
 */
public class DeviceGroupVO {

	/**
	 * 元服务Id
	 */
	private Integer metaServiceId;
	
	/**
	 * 设备类型
	 */
	private DeviceTypeVO deviceType;
	
	/**
	 * 设备信息
	 */
	private List<DeviceVO> devices;

	/**
	 * @return DeviceTypeVO 取得域 deviceType 的方法。
	 */
	public DeviceTypeVO getDeviceType() {
		return deviceType;
	}

	/**
	 * @param deviceType 对域 deviceType 的设置方法.
	 */
	public void setDeviceType(DeviceTypeVO deviceType) {
		this.deviceType = deviceType;
	}

	/**
	 * @return List<DeviceVO> 取得域 devices 的方法。
	 */
	public List<DeviceVO> getDevices() {
		return devices;
	}

	/**
	 * @param devices 对域 devices 的设置方法.
	 */
	public void setDevices(List<DeviceVO> devices) {
		this.devices = devices;
	}

	/**
	 * @return Integer 取得域 metaServiceId 的方法。
	 */
	public Integer getMetaServiceId() {
		return metaServiceId;
	}

	/**
	 * @param metaServiceId 对域 metaServiceId 的设置方法.
	 */
	public void setMetaServiceId(Integer metaServiceId) {
		this.metaServiceId = metaServiceId;
	}
}
