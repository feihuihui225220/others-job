@charset "utf-8";
/*通用*/
.wrapper{clear: both;width: 100%; }
.main{clear: both;}
body {
	font: 12px "microsoft yahei", Arial, Helvetica, sans-serif;
    color:#151515;
    margin:0px;
    padding:0px;
}
ul,li,p,h1,h2,h3,h4,h5,h6,dl,dt,dd {
    margin:0px;
    padding:0px;
    border:none;
    list-style:none;
}
input {}
.fl   {float:left;}
.fr  {float:right;}
.clear  {clear:both;}
a  {
    color:#666;
    text-decoration:none;
}
