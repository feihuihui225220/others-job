body,nav,dl,dt,dd,p,h1,h2,h3,h4,ul,ol,li,input,button,textarea,footer{margin:0;padding:0}
html, body {width:100%;height:100%;}
body{font:16px/1.5 'Microsoft Yahei','Simsun';  color:#333;-webkit-text-size-adjust: none; min-width:320px;background:#fffee0;}
body.reg {background:#fffee0 url(img/bg.jpg) no-repeat 0 0;background-size:100% 100%;}
h1,h2,h3,h4,h5,h6{font-size:100%}
form{display:inline}
ul,ol{list-style:none}
a{text-decoration:none;color:#1a1a1a}
a:hover, a:active, a:focus{color:#1c5aa2;text-decoration: none;}
a:active{color:#aaa;}
img{vertical-align:middle;border:0;-ms-interpolation-mode:bicubic;}
button,input,select,textarea{font-size:100%; vertical-align:middle;outline:none;}
textarea{resize:none}
button,input[type="button"],input[type="reset"],input[type="submit"] {cursor:pointer;-webkit-appearance:button;-moz-appearance:button}
input:focus:-moz-placeholder,input:focus::-webkit-input-placeholder {color:transparent}
button::-moz-focus-inner,input::-moz-focus-inner { padding:0; border:0}
table { border-collapse:collapse; border-spacing:0}
.fl{float:left;}.fr{float:right;}.hide{display:none;}.show{display: block;}
.ellipsis { white-space:nowrap; text-overflow:ellipsis; overflow:hidden}
.break { word-break:break-all; word-wrap:break-word}
.wrapper {width:320px;margin:0 auto;}
.top_header {height:39px;line-height:39px;border-bottom:1px solid #ac1403;color:#fff;font-size:18px;text-align:center;padding:0 13px;background:#cf3313 url(img/nav.png) repeat-x 0 0;position:relative;}
.top_header a {position:absolute;text-indent:-9999em;width:24px;height:24px;background-size:100%;background-repeat:no-repeat;background-position:0 0;margin-top:7px;}
.top_header .set {right:15px;background-image:url(img/ico_set.png);display:none;}
.top_header .home {left:15px;background-image:url(img/ico_home.png);}
.top_header .sel_all {right:15px;;text-indent:0;font-size:16px;color:#fff;width:auto;font-family:-webkit-body;margin-top:0;display:none;}
.top_header .back {left:15px;text-indent:0;font-size:16px;color:#fff;width:auto;font-family:-webkit-body;margin-top:0;}
.box {padding:20px;background:#fffed6;border-bottom:1px solid #cdab63;margin-bottom:5px;}
.con {background:#f0c974;}
.box header {height:20px;line-height:20px;color:#515567;border-bottom:1px solid #eae9c4;padding-bottom:5px;background-size:21px 18px;padding-left:30px;background-position:2px 2px;background-repeat:no-repeat;}
.pad5 {padding:5px;}


/* 注册登录 */
.login {margin:25px 13px 0;}
.login header {height:30px;line-height:30px;border-bottom:1px solid #c85940;font-size:16px;padding-left:30px;background:url(img/logo.png) no-repeat 0 50%;background-size:23px 25px;color:#fff;margin-bottom:20px;padding-bottom:5px;}
.login form {display:block;text-align:center;}
.login input {height:20px;line-height:20px;border-radius:5px;border:0 none;font-size:16px;padding:7px 6px 6px 20px;margin-bottom:25px;width:62%;}
.login label {display:inline-block;text-align:right;width:5em;color:#fff;height:20px;line-height:20px;position:relative;top:-10px}
.login button, .submit {width:60%;height:40px;line-height:40px;border-radius:10px;border:1px solid #327000;background:#3c8601;color:#fff;font-size:16px;display:block;margin:0 auto;}
.login button:active, .submit:active {background:#6BC226;}



/* 订单详情 */
.order_meta {padding:15px 20px;background:#4b8209;line-height:1.8;color:#fff;position:relative;height:84px;}
.order_meta .score {position:absolute;top:50%;right:25px;margin-top:-20px;padding:6px 12px;background:#ea8105;}
.box .ico_list {background-image:url(img/ico_list.png);}
.box .ico_user {background-image:url(img/ico_user.png);background-size:16px 19px;}
.box .ico_time {background-image:url(img/ico_time.png);background-size:18px 18px;}
.list {padding-top:15px;}
.list li {margin-bottom:8px;}
.list li span {float:right;}

.do_score {display:block;padding:20px;background:#f2f2f2;border-top:1px solid #de2043;text-align:center;line-height:1.8;position:fixed;bottom:0;width:100%;display:none;}
.do_score i {display:inline-block;width:250px;height:50px;}
.do_score i s {width:50px;height:50px;display:inline-block;background:url(img/star_off_32.png) no-repeat 50%;}
.do_score i .on {background:url(img/star_32.png) no-repeat 50% 50%;}
.do_score button {padding:5px 10px;margin:0 10px;}



/* 订购服务 */
.tab_tit {padding:2px;background:#44bf03;margin-bottom:10px;}
.tab_tit a {width:50%;display:inline-block;text-align:center;height:32px;line-height:32px;color:#44bf03;background:#fff;}
.tab_tit .act {background:#44bf03;color:#fff;position:relative;}
.tab_tit .act:after {content:'';width:0;height:0;border-left:8px solid transparent;border-right:8px solid transparent;border-bottom:8px solid #fff;position:absolute;left:50%;margin-left:-8px;bottom:-2px;}

.list_server a {display:block;margin-bottom:10px;height:90px;line-height:90px;font-family:-webkit-body;text-align:right;font-weight:bold;color:#893103;letter-spacing:3px;background:#fce585 url(img/ico_b_heart3.png) no-repeat -5px -5px;background-size:200px 100px;}
.list_server a:nth-child(even) {background-color:#fa9605;color:#fff;}

/* 我的订单 */
.list_my_server a {display:block;margin-bottom:10px;height:100px;font-family:-webkit-body;color:#893103;background:#fce585 url(img/ico_b_s3.png) no-repeat 15px 50%;background-size:60px 60px;padding:10px 0 10px 100px;position:relative;}
.list_my_server a h4 {font-size:14px;margin-bottom:6px;}
.list_my_server a p {font-size:12px;}
.list_my_server a:nth-child(even) {background:#fa9605 url(img/ico_b_s1.png) no-repeat 15px 50%;;color:#fff;background-size:60px 60px;}
.list_my_server span {position:relative;left:96%;top:-60px;font-size:22px;}



/* 公告列表 */
.list_notice li {padding:15px;background:#fff url(img/bg_yellor.png) repeat-x 0 0;border-bottom:1px solid #f5e18c;}
.list_notice h4 {color:#b11105;margin-bottom:5px;font-size:16px;}
.list_notice h4 span {float:right;font-weight:normal;font-size:14px;}
.list_notice p {text-indent:2em;font-size:14px;color:#755206;-webkit-line-clamp:2;height:42px;overflow:hidden;-webkit-box-orient:vertical;display:-webkit-box;}


/* 公告详情 */
.notice_con header {height:40px;line-height:40px;padding:0 20px;background:#ffed72;font-size:16px;}
.notice_con header span {float:right;font-size:12px;color:#666;}
.notice_con {padding:20px;font-size:14px;}
.notice_con div {padding:15px;background:#fcfee0;}
.notice_con div div {margin-bottom:15px 15px;}
.notice_con p {margin-bottom:15px;}



/* 确认订单 */
.sel_time {padding:20px 0 20px 40px;background:url(img/ico_time.png) no-repeat 15px 50%;background-size:18px 18px;height:40px;line-height:40px;border-bottom:1px solid #f0c974;}
.user_input {padding:20px;border-bottom:1px solid #f0c974;}
.user_input header {margin-bottom:15px;background:url(img/ico_user.png) no-repeat 0 50%;padding-left:25px;background-size:16px 20px}
.user_input li {margin-bottom:15px;}
.user_input li:nth-child(3) input {width:80%;}
.submit {margin:30px auto 0; padding-bottom:30px;}
.order_form {display:block;padding-bottom:30px;}



/* 首页 */
.index_btn {display:block;margin:20px;height:90px;line-height:90px;background:#44bf04 url(img/ico_b_flower.png) no-repeat 20px 50%;background-size:38px 50px;color:#fff;font-size:36px;text-align:center;letter-spacing:3px;}
.index_btn:active {color:#1c5aa2;}
.index_btn:nth-child(1) {background-image:url(img/ico_b_umbrella.png)}
.index_btn span {float:right;height:90px;line-height:90px;font-size:40px;font-family:-webkit-body;margin-right:15px;}


/* 选择菜单 */
.sel_order li {padding:20px 15px;border-bottom:1px solid #f0c974;text-align:center;}
.sel_order li p {text-align:center;width:7em;position:absolute;left:0;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;}
.sel_order li p span {display:block;text-align:center;color:#b6533e;}
.sel_order li .com {display:inline-block;}
.sel_order li a {position:absolute;right:15px;width:32px;height:32px;background:url(img/ico_cir.png) no-repeat 0 0;background-size:32px 32px;-webkit-tap-highlight-color: transparent;}
.sel_order li .on {position:absolute;right:15px;width:32px;height:32px;background:url(img/ico_cir_yes.png) no-repeat 0 0;background-size:32px 32px;}
.sel_order .com input {width:2em;text-align:center;height:32px;font-size:18px;line-height:32px;vertical-align:top;margin:0 10px;border:2px solid #5f646e;}
.sel_order .com i {display:inline-block;text-align:center;font-size:32px;width:32px;height:32px;line-height:32px;border:2px solid #5f646e;border-radius:5px;background:url(img/ico_-.png) no-repeat 50%;}
.sel_order .com b {display:inline-block;text-align:center;font-size:32px;width:32px;height:32px;line-height:32px;border:2px solid #5f646e;border-radius:5px;background:url(img/ico_+.png) no-repeat 50%;}



/* 选择服务商 */
.list_pic li {background:#fff;margin-bottom:8px;border-top:1px solid #ddd;border-bottom:1px solid #ddd;}
.list_pic a {display:block;padding:15px 20px;height:90px;}
.list_pic img {float:left;margin-right:15px;width:120px;height:90px;border-radius:5px;}
.list_pic h4 {margin-bottom:15px;}



/* 关爱服务 */
.fix_guanai a {background-image:url(img/ico_b_s4.png);height:120px;}
.fix_guanai a:nth-child(even) {height:120px;background-image:url(img/ico_b_s2.png);}
.fix_guanai span {top:-70px}
.fix_guanai_2 {height:110px;}
.fix_guanai_3 select {margin-top:20px;}
.fix_guanai_3 button {margin-bottom:30px;}
.fix_guanai_0 {padding-bottom:30px;display:block;}



/* 专业服务 */
.fix_zhuanye a {background-image:url(img/ico_b_alert.png);height:110px;}
.fix_zhuanye a:nth-child(even) {background-image:url(img/ico_b_alert.png);height:110px;}
.fix_zhuanye span {top:-70px}
.box .ico_alert {background-image:url(img/ico_alert.png);background-size:18px 18px}
.box .ico_edit {background-image:url(img/ico_edit.png);background-size:18px 18px}
.box textarea {width:86%;height:60px;display:block;margin-top:20px;padding:5px;}

.fix_zhuanye_1 a {background-image:url(img/ico_b_user.png);}
.fix_zhuanye_1 a:nth-child(even) {background-image:url(img/ico_b_user.png);}

.fix_zhuanye_2 {padding:20px;display:block;}
.fix_zhuanye_2 input {padding:5px;height:20px;line-height:20px;font-size:18px;}
.fix_zhuanye_2 button {height:34px;padding:0 10px;margin-left:15px;}

.box .ico_heart {background-image:url(img/ico_heart.png);background-size:18px 18px}
.box .ico_heart {background-image:url(img/ico_heart.png);background-size:18px 18px}
.box .ico_heart a {float:right;background:#44bf04;padding:3px 10px;border:1px solid #389e02;font-size:14px;border-radius:5px;color:#fff;margin-top:-8px;}

.list_2 li {padding:15px 0;border-bottom:1px solid #eae9c4;}
.list_2 span {display:inline-block;width:80px;text-align:right;}
.fix_zhuanye_0 s {display:block;height:20px;}



/* 老年人专业服务列表 */
.fix_pro a {height:80px;}
.fix_pro span {top:-55px}


/* 后加 */
p.done b{color:#666;font-weight:normal;}