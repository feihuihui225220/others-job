 function sel_div(t)
        {
         for(var i=0;i<t.length;i++)
         {
          document.getElementById(t.options[i].value).style.display="none";
         }
         if(t.value!="请选择")
         {
          document.getElementById(t.value).style.display="block";
         }
        }

