    </div>
    </div>
    </div>
</div>
</body>
 <script>
   // 文本框不能为空且长度不小于30
    $('.button_tj').click(function() {
        var linshi = 0;
        var sum = $(".sum").val();
        var con1 = $('.content1');

        for(var a = 1;a < 17;a++){
            //jquery选择器
            if($('input[name=optionsRadios'+a+']:checked').length==0){
                linshi = 1;
            }
        }
        if(linshi==0){
            con1.hide();
            if(sum.length<=30){
                alert("问题总结长度不能小于30");
                return false;
            }else {
                con1.show();
            }
        }
        else{
            alert("请将所有题目全部答完！");
            return false;
        }
          // 遍历出所选选项class="checxked"
        $('.checxked').each(function(){
            if($(this).prop('checked')){
                $(this).attr("checked","true");
            }
        });
        $('.form-control').text($('.form-control').val());
        $('#detail').val($('#assess').html());
        $('#from_1').submit();
    });




</script>
  
</html>