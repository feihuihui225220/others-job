body{
	margin:0; padding:0;
	background-color: #D9EBEB ;
	background-position:  center  center;
	background-repeat:no-repeat ;
	width:100%; height:100%;
	font-family: "微软雅黑"; 
}
/**url(img/body_bg.jpg)*/

.nav{
	height:50px;
	background-color: #16A5AD;
	font-family: "微软雅黑";
	font-size: 20px;
	font-weight: bold;
	color: #FFFFFF;
	line-height: 50px;
	padding: 0 20px;
}
.main_register{background: #FFFFFF;width: 410px;height: 415px;margin: auto; margin-top: 100px;
 }
 .main_login{background: #FFFFFF;width: 410px;height: 300px;margin: auto; margin-top: 100px;
 }
 .main_login .regiser-position{ position:relative; left:100px;float:  left;width: 88px;}
 .main_login .login-position{float: left;width: 88px;}
.logintop{width: 410px;height: 55px;line-height:55px; color:#FFFFFF;font-weight:bold;background: #16A5AD;text-align: center;margin-top:0px;}
.login{width: 410px;margin: auto;font-size: 14pt; background: #fff;text-align: left;margin-top:0px;}
.main_login .login label{text-align: left;display:inline-block;width:75px;}
.main_register .login label{text-align: left;display:inline-block;width:95px;}
.main_register .regiser-position{ position:relative; left:100px;float:  left;width: 88px;}
.main_register .login-position{float: left;width: 88px;}
.login .input{width: 200px;height: 26px;}
.login select{width: 100px;height: 26px;}
.login .safe{width: 70px;height: 26px;}
.login ul{margin:7px 0px;}
.login button{background-color: #d44919; width: 141px; height: 35px;  color: #FFFFFF; font-size: 14px;margin:20px 135px; 
	border: 0px; 
	border-radius: 2px;
	font-weight: bold;
}
.login .loginbtn{cursor:pointer;background-color: #4AC6CE; width: 88px; height: 35px;  color: #FFFFFF; 
	font-size: 14px;
	margin:20px 80px; 
	border: 0px; 
	border-radius: 5px;
}
.login .registerbtn{
	cursor:pointer;
	background-color: #7A9292; 
	width: 88px; 
	height: 35px;  
	color: #FFFFFF; 
	font-size: 14px;
	margin:20px 60px; 
	border: 0px; 
	border-radius: 5px;
}

