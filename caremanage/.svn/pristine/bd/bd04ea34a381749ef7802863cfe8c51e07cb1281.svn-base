		this.idCard = idCard;
	}

	/**
	 * @return Integer 取得域 catId 的方法。
	 */
	public Integer getCatId() {
		return catId;
	}

	/**
	 * @param catId 对域 catId 的设置方法.
	 */
	public void setCatId(Integer catId) {
		this.catId = catId;
	}

	/**
