package com.anticw.aged.vo.professional.recommand;

import java.util.List;

/**
 * 推荐vo
 * <P>File name : RecommandVO.java </P>
 * <P>Author : zhouyanxin </P> 
 * <P>Date : 2014-10-5 </P>
 */
public class RecommandVO {

	/**
	 * 姓名
	 */
	private String name;
	
	/**
	 * 身份证号码
	 */
	private String idCard;
	
	/**
	 * 联系方式
	 */
	private String concatPhone;
	
	/**
	 * 推荐服务列表
	 */
	private List<ServiceExVO> services;

	/**
	 * @return String 取得域 name 的方法。
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name 对域 name 的设置方法.
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @return String 取得域 idCard 的方法。
	 */
	public String getIdCard() {
		return idCard;
	}

	/**
	 * @param idCard 对域 idCard 的设置方法.
	 */
	public void setIdCard(String idCard) {
		this.idCard = idCard;
	}

	/**
	 * @return String 取得域 concatPhone 的方法。
	 */
	public String getConcatPhone() {
		return concatPhone;
	}

	/**
	 * @param concatPhone 对域 concatPhone 的设置方法.
	 */
	public void setConcatPhone(String concatPhone) {
		this.concatPhone = concatPhone;
	}

	/**
	 * @return List<ServiceVO> 取得域 services 的方法。
	 */
	public List<ServiceExVO> getServices() {
		return services;
	}

	/**
	 * @param services 对域 services 的设置方法.
	 */
	public void setServices(List<ServiceExVO> services) {
		this.services = services;
	}
}
