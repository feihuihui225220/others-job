package com.anticw.aged.bean.gds;

public class BpReport
{
	private boolean	rfok;			//患者目前状况指标，用来指示4条线的含义
	private String	summary;			//要点
	private String	attention;		//注意
	private String	details;			//详解
	private String	education;		//健康教育
	
	public boolean	getRfOK() 		{ return rfok; }
	public String	getSummary() 	{ return summary; }
	public String	getAttention()	{ return attention; }
	public String	getDetails()		{ return details; }
	public String	getEducation()	{ return education; }

	public void		setRfOK(boolean b)		{ rfok = b; }
	public void		setSummary(String s)		{ summary = s; }
	public void		setAttention(String s)	{ attention = s; }
	public void		setDetails(String s)		{ details = s; }
	public void		setEducation(String s)	{ education = s; }
}